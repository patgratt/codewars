    "def bmi(weight, height):\n",
    "    result = weight / height**2\n",
    "    if 25 < result <= 30:\n",
    "        return 'Overweight'\n",
    "    elif 18.5 < result <= 25:\n",
    "        return 'Normal'\n",
    "    elif result <= 18.5:\n",
    "        return 'Underweight'\n",
    "    else:\n",
    "        return 'Obese'"
   ]
