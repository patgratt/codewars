   "source": [
    "ex = ['1:0','2:0','3:0','4:0','2:1','3:1','4:1','3:2','4:2','4:3']\n",
    "\n",
    "def points(games):\n",
    "    res = 0\n",
    "\n",
    "    for game in games:\n",
    "        if game[0] > game[2]:\n",
    "            res += 3\n",
    "        elif game[0] == game[2]:\n",
    "            res += 1\n",
    "    return res"
   ]
