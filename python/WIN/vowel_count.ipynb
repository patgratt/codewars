   "source": [
    "def get_count(sentence):\n",
    "    vowel_list = ['a','e','i','o','u']\n",
    "    count = 0\n",
    "    for letter in sentence:\n",
    "        if letter in vowel_list:\n",
    "            count += 1\n",
    "    return count"
   ]
